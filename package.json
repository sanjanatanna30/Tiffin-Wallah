    "mern",
    "food-delivery",
    "tiffin"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "dependencies": {
    "mongodb": "^6.16.0",
    "react-icons": "^5.5.0"
  }
}
